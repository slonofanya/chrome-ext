{
  "name": "Twich watch helper",
  "description": "Help me to watch twitch streams.",
  "version": "1",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "optional_host_permissions": [],
  "content_scripts": [
    {
      "matches": [
        "https://www.twitch.tv/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "manifest_version": 3,
  "icons": {
    "16": "./cloudflare.png",
    "32": "./cloudflare.png",
    "48": "./cloudflare.png",
    "128": "./cloudflare.png"
  }
}