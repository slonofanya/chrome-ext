{
  "name": "SL",
  "description": "Some usedull things",
  "version": "0.0",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }],
  "manifest_version": 2,
  "icons": {
    "16": "./cloudflare.png",
    "32": "./cloudflare.png",
    "48": "./cloudflare.png",
    "128": "./cloudflare.png"
  }
}
